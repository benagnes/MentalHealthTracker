<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mentalhealthtracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MentalHealthTracker">
        <activity android:name="com.example.mentalhealthtracker.Yoga"></activity>
        <activity android:name="com.example.mentalhealthtracker.MoodTracker"></activity>
        <activity android:name="com.example.mentalhealthtracker.BreathingExercises"></activity>
        <activity android:name="com.example.mentalhealthtracker.Mindfulness"></activity>
        <activity android:name="com.example.mentalhealthtracker.WebSupportArticles"></activity>
        <activity android:name="com.example.mentalhealthtracker.MoreResources"></activity>
        <activity
            android:name="com.example.mentalhealthtracker.music.PlayMusic"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity
            android:name="com.example.mentalhealthtracker.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.MentalHealthTracker.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>